{
  tables: [{
      name: PlayerStats
      sql: PlayerStats
      schema: gamedb
      cardinality : large
      hidden : false
      readAccess : A user is admin or is a player in the game
      joins: [
          {
             name: playerCountry
             to: country
             type: toOne
             definition: '%Join.id = {{this}}.country_id'
          }
      ]
      dimensions : [
         {
           name : countryIsoCode
           type : TEXT
           definition : '{{playerCountry.isoCode}}'
         }
      ]
  }]
}
